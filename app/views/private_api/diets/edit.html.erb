<h2>Edit your diet</h2>
<%= form_for @diet do |f| %>
  <%= f.label :name, "Your diet name: " %>
  <%= f.text_field :name%>

  <%= f.label :description, "Description: " %>
  <%= f.text_field :description %>

  <%= f.label :components, "Components: " %>
  <%= f.text_field :components %>

  <%= f.submit "Update Your Diet" %>
<% end %>
